{
  "name": "ngx-svg-schematic",
  "version": "1.2.3",
  "description": "A schematic to generate SVG components from SVG files; making it easier to embed SVGs in templates and avoid conflicting CSS rules from multiple embedded SVGs.",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "test": "npm run build && jasmine src/**/*_spec.js"
  },
  "keywords": [
    "schematics",
    "angular-schematics",
    "angular-svg-icon",
    "svg",
    "angular-svg",
    "angular-svg-component",
    "ngx-svg-schematic"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Teebo/ngx-svg-schematic.git"
  },
  "bugs": {
    "url": "https://github.com/Teebo/ngx-svg-schematic.git"
  },
  "homepage": "https://github.com/Teebo/ngx-svg-schematic.git",
  "author": "Thabo Ambrose <ngubanethabo.ambrose@gmail.com>",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^12.1.4",
    "@angular-devkit/schematics": "^12.1.4",
    "@schematics/angular": "^12.1.4",
    "jsdom": "^17.0.0",
    "typescript": "~4.3.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "jasmine": "^3.5.0"
  },
  "ng-add": {
    "save": "dependencies"
  }
}
